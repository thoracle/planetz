{
  "template_id": "delivery",
  "title": "Cargo Delivery Contract",
  "description": "Transport cargo safely to the specified destination",
  "mission_type": "delivery",
  "faction": "terran_republic_alliance",
  "reward_package_id": 4,
  "objectives": [
    {
      "id": "1",
      "description": "Load {cargo_amount} units of {cargo_type}",
      "is_optional": false,
      "is_ordered": true
    },
    {
      "id": "2",
      "description": "Deliver cargo to {destination}",
      "is_optional": false,
      "is_ordered": true
    },
    {
      "id": "3",
      "description": "Maintain cargo integrity above {min_integrity}%",
      "is_optional": true,
      "is_ordered": false
    }
  ],
  "custom_fields": {
    "cargo_type": "medical_supplies",
    "cargo_amount": 50,
    "destination": "europa_station",
    "pickup_location": "terra_prime",
    "min_integrity": 90,
    "time_limit": 3600,
    "delivery_bonus": 800,
    "integrity_bonus": 200
  },
  "triggers": {
    "on_accept": "notify_cargo_coordinator",
    "on_cargo_loaded": "begin_transport_mission",
    "on_cargo_delivered": "verify_delivery_completion",
    "on_complete": "award_delivery_bonus"
  },
  "location_variants": {
    "terra_prime": {
      "title": "Medical Supply Run",
      "description": "Emergency medical supplies needed at Europa Station",
      "custom_fields": {
        "cargo_type": "medical_supplies",
        "destination": "europa_station",
        "time_limit": 1800,
        "delivery_bonus": 1000,
        "urgency_level": "high"
      }
    },
    "europa_station": {
      "title": "Raw Materials Transport",
      "description": "Industrial materials needed at Ceres Outpost",
      "custom_fields": {
        "cargo_type": "raw_materials",
        "cargo_amount": 100,
        "destination": "ceres_outpost",
        "delivery_bonus": 600,
        "urgency_level": "medium"
      }
    },
    "ceres_outpost": {
      "title": "Food Supply Mission",
      "description": "Emergency food rations for Mars Base",
      "custom_fields": {
        "cargo_type": "food_rations",
        "cargo_amount": 75,
        "destination": "mars_base",
        "time_limit": 2400,
        "delivery_bonus": 500,
        "urgency_level": "medium"
      }
    }
  },
  "level_scaling": {
    "cargo_amount_by_level": {
      "1-5": [25, 50, 75],
      "6-10": [50, 100, 150],
      "11-15": [100, 200, 300],
      "16-20": [200, 400, 500]
    },
    "reward_scaling": {
      "base_credits": 800,
      "credits_per_level": 80,
      "distance_multiplier": 1.5
    }
  },
  "random_elements": {
    "cargo_type": [
      "medical_supplies",
      "food_rations", 
      "raw_materials"
    ],
    "destination": [
      "europa_station",
      "ceres_outpost",
      "mars_base",
      "luna_port"
    ],
    "cargo_amount": [25, 50, 75, 100],
    "min_integrity": [85, 90, 95],
    "delivery_bonus": [500, 600, 700, 800, 900, 1000]
  },
  "failure_conditions": {
    "cargo_lost": {
      "botch_mission": true,
      "reputation_penalty": -30,
      "faction_standing_loss": -20
    },
    "integrity_too_low": {
      "reduce_payment": 0.5,
      "reputation_penalty": -10,
      "no_integrity_bonus": true
    },
    "time_limit_exceeded": {
      "reduce_payment": 0.7,
      "reputation_penalty": -15,
      "no_delivery_bonus": true
    }
  },
  "cargo_requirements": {
    "required_cargo_capacity": 100,
    "recommended_hold_features": ["environmental_control"],
    "contraband_check": false,
    "special_handling": {
      "medical_supplies": {
        "temperature_controlled": true,
        "fragile": true,
        "decay_rate": 0.1
      },
      "food_rations": {
        "temperature_controlled": false,
        "fragile": false,
        "decay_rate": 0.05
      },
      "raw_materials": {
        "temperature_controlled": false,
        "fragile": false,
        "decay_rate": 0.01
      }
    }
  },
  "cascade_effects": [
    {
      "trigger": "cargo_lost",
      "effects": [
        {
          "type": "modify_faction_standing",
          "faction": "terran_republic_alliance",
          "change": -15
        },
        {
          "type": "spawn_emergency_mission",
          "mission_type": "replacement_delivery"
        }
      ]
    },
    {
      "trigger": "perfect_delivery",
      "effects": [
        {
          "type": "modify_faction_standing", 
          "faction": "terran_republic_alliance",
          "change": 10
        },
        {
          "type": "unlock_bonus_mission",
          "mission_id": "high_value_transport"
        }
      ]
    }
  ]
}